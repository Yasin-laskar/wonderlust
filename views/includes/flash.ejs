

<% if (message && message.length) { %>
    <div class="alert alert-success alert-dismissible fade show col-md-6 offset-md-1" role="alert">
      <h5><%= message %></h5>
      <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
  </div>
 <% } %>

<% if (wrong && wrong.length) { %>
    <div class="alert alert-success alert-dismissible fade show col-md-6 offset-md-2" role="alert">
      <h5><%= wrong %></h5>
      <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
  </div>
 <% } %>


